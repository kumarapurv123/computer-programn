#include<stdio.h>
int main()
{
    char str[20];
    int i,c1=0,c2=0,c3=0,c4=0;
    printf("enter the string");
    gets (str);
    for(i=0;str[i]!='\0';i++)
    {
    
        if (str[i]>=65 && str[i]<=90)
        c1++;
        else
        if(str[i]>=97 && str[i]<=122)
        c2++;
        else
        if(str[i]>=48 && str[i]<=57)
        c3++;
        else
        c4++;
    }
    printf("upper case=%d",c1);
    printf("lower case=%d",c2);
    printf("digit number=%d",c3);
    printf("special symbol=%d",c4);
}
